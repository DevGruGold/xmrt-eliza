{
  "version": 2,
  "name": "xmrt-eliza-simple",
  "framework": null,
  "buildCommand": "echo 'Simple build complete - no compilation needed'",
  "outputDirectory": "./",
  "installCommand": "echo 'No installation needed - using built-in Node.js modules'",
  "devCommand": "node index.js",
  "ignoreCommand": "exit 0;",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "10mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "functions": {
    "index.js": {
      "memory": 512,
      "maxDuration": 10
    }
  }
}