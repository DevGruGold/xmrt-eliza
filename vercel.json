{
  "version": 2,
  "name": "xmrt-eliza",
  "framework": null,
  "buildCommand": "echo 'Skipping complex build, using simple setup'",
  "outputDirectory": "./",
  "installCommand": "npm install express cors axios",
  "devCommand": "node index.js",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "functions": {
    "index.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}