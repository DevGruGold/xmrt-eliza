{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".vercel/**"
      ],
      "env": [
        "NODE_ENV",
        "XMRT_AGENT_ID",
        "XMRT_SUPABASE_URL",
        "XMRT_ECOSYSTEM_URL",
        "XMRT_SUITE_AI_URL",
        "OPENAI_API_KEY"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "dependsOn": [
        "^build"
      ]
    }
  }
}